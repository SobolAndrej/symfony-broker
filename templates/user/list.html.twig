{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.balance }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}